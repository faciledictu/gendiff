{
  "plugins": [
    "jest"
  ],
  "env": {
    "node": true
  },
  "extends": [
    "plugin:jest/recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "no-console" : "off",
    "no-underscore-dangle": [
      2,
      {
        "allow": [
          "__filename",
          "__dirname"
        ]
      }
    ]
  }
}
